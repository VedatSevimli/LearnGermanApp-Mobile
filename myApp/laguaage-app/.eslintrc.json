{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent":["error",4],
        "no-await-in-loop":"error",
        "no-console":"warn",
        "no-unused-vars":"off",
        "no-var":"error",
        "no-nested-ternary":"error",
        "quotes":["error","single"],
        "semi":["error","always"],
        "eqeqeq":"error"
    },
    "settings": {
        "react": {
          "version": "detect"
        }
      }
}
